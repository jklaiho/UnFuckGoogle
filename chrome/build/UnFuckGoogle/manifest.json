{
    "name": "UnFuckGoogle",
    "version": "1.2",
    "description": "Removes various annoyances from Google",
    "update_url": "http://jklaiho.net/UnFuckGoogle/chrome/updates.xml",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "include_globs": ["http*://*.google.*/*"],
            "js": ["start.js"],
            "css": ["UnFuckGoogle.css"],
            "run_at": "document_end"
        },
        {
            "matches": ["<all_urls>"],
            "include_globs": ["http*://*.google.*/*"],
            "js": ["end.js"],
            "run_at": "document_idle"
        }
    ]
}
