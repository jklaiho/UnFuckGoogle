{
    "name": "UnFuckGoogle",
    "version": "1.2.2",
    "description": "Removes various annoyances from Google",
    "update_url": "http://files.jklaiho.net/UnFuckGoogle/chrome/updates.xml",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "include_globs": ["http*://*.google.*/*"],
            "css": ["UnFuckGoogle.css"],
            "run_at": "document_end"
        },
        {
            "matches": ["<all_urls>"],
            "include_globs": ["http*://*.google.*/*"],
            "js": ["UnFuckGoogle.js"],
            "run_at": "document_idle"
        }
    ]
}
